{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"],
        "experimentalEsm": true
      }
    }
  ],
  "routes": [
    {
      "src": "/models/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/database/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
